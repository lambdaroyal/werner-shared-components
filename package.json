{
  "name": "werner-shared-components",
  "description": "shared components for werner-pwa and werner-server",
  "version": "1.1.36",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    ".": {
      "solid": {
        "development": "./dist/dev.jsx",
        "import": "./dist/index.jsx"
      },
      "development": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/dev.js"
        }
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm run build",
    "release": "release-it"
  },
  "dependencies": {
    "clsx": "^1.2.0"
  },
  "peerDependencies": {
    "solid-js": "^1.8.18"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "^5.0.0",
    "@rollup/plugin-replace": "^5.0.7",
    "@swc/core": "^1.6.13",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "daisyui": "^4.12.10",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "release-it": "^17.6.0",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.77.6",
    "tailwindcss": "^3.4.5",
    "tsup": "^8.1.1",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.5.3"
  }
}